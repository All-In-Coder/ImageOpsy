<div class="pageContentWrapper">
  <form action="/cnnmrf" enctype="multipart/form-data" method="POST">

  <div class="first_original">
      <input type="file" 
    accept="image/*"
    name="image"
    class="file" />
  </div>
  <div class="plus">
    <i class="fas fa-plus plusSign"></i>
  </div>
  <div class="second_original">
     <input type="file"
    accept="image/*"
    name="image"
    class="file"
    placeholder="Ok"
     />
  </div>
 
    <div class="submit">
        <button type="submit" id="submit">Submit</button>
    </div>
    
    
    
    
  </form>
</div>

<div class="ans">
  {{# if img}}
  <img src="{{img}}" alt="..." srcset="" />
  {{/if}} {{# if err}}
  <p>{{err}}</p>
  {{/if}}
</div>
